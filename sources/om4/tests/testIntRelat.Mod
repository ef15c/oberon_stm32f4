MODULE testIntRelat;
    
PROCEDURE p;
    VAR a, b: INTEGER;
      t: BOOLEAN;
BEGIN
  t := a>1;
  t := a>=2;
  t := a=3;
  t := a<-1;
  t := a<=-2;
  t := a#-3;
  t := 1>a;
  t := 2>=a;
  t := 3=a;
  t := -1<a;
  t := -2<=a;
  t := -3#a;
  t := a>b;
  t := a>=b;
  t := a=b;
  t := a<b;
  t := a<=b;
  t := a#b
END p;

END testIntRelat.

