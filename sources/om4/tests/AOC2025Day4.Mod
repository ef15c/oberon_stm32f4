MODULE  AOC2025Day4;
  IMPORT Texts, Oberon, L := SystemLog;

  CONST size = 140;

  TYPE ProcessProc = PROCEDURE(h, w: INTEGER);
    Map = ARRAY size, size OF CHAR;

  VAR W: Texts.Writer;
    map: Map;
    nb: INTEGER;

  PROCEDURE parse(process: ProcessProc);
    VAR S!: Texts.Scanner; ch: CHAR; i, j, w: INTEGER;
  BEGIN
    Texts.OpenScanner(S, Oberon.Par.text, Oberon.Par.pos);
    Texts.Read(S, ch); WHILE ch = Texts.CR DO Texts.Read(S, ch) END;
    w := 0; i := 0; j := 0;
    WHILE (ch = ".") OR (ch = "@") OR (ch = Texts.CR) DO
      IF ch = Texts.CR THEN INC(i); IF w = 0 THEN w := j ELSE ASSERT(w = j) END; j := 0
      ELSE map[i, j] := ch; INC(j)
      END;
      Texts.Read(S, ch)
    END;
    process(i, w)
  END parse;

  PROCEDURE accessible(h, w, i, j: INTEGER): BOOLEAN;
    VAR nbf, k, l, m, n: INTEGER;
  BEGIN nbf := 0;
    FOR k := -1 TO 1 DO
      FOR l := -1 TO 1 DO
        m := i + k; n := j + l;
        IF ((k # 0) OR (l # 0))
          & (m >= 0) & (m < h) & (n >= 0) & (n < w) & (map[m, n] = "@") THEN INC(nbf)
        END
      END
    END

    RETURN nbf < 4
  END accessible;

  PROCEDURE doPart1(h, w: INTEGER);
    VAR i, j: INTEGER;
  BEGIN
    FOR i := 0 TO h - 1 DO
      FOR j := 0 TO w - 1 DO
        IF (map[i, j] = "@") & accessible(h, w, i, j) THEN INC(nb) END
      END
    END
  END doPart1;

  PROCEDURE doPart2(h, w: INTEGER);
    VAR i, j, np: INTEGER;
  BEGIN
    REPEAT np := nb; LED(np);
      FOR i := 0 TO h - 1 DO
        FOR j := 0 TO w - 1 DO
          IF (map[i, j] = "@") & accessible(h, w, i, j) THEN
            INC(nb); map[i, j] := "."
          END
        END
      END
    UNTIL np = nb
  END doPart2;

  PROCEDURE part1*;
  BEGIN
    nb := 0; parse(doPart1);
    Texts.WriteString(W, "Partie 1: nb = "); Texts.WriteInt(W, nb, 1); Texts.WriteLn(W);
    Texts.Append(Oberon.Log, W.buf)
  END part1;

  PROCEDURE part2*;
  BEGIN
    nb := 0; parse(doPart2);
    Texts.WriteString(W, "Partie 2: nb = "); Texts.WriteInt(W, nb, 1); Texts.WriteLn(W);
    Texts.Append(Oberon.Log, W.buf)
  END part2;

BEGIN Texts.OpenWriter(W)
END AOC2025Day4.

