MODULE AOC2025Day6;
  IMPORT Texts, Oberon, U := AOCUtils, L := SystemLog;

  TYPE ProcessProc = PROCEDURE;
    Problem = POINTER TO ProblemDesc;
    ProblemDesc = RECORD opr: CHAR; opd: ARRAY 5 OF LONGINT; nbd: INTEGER;
      next: Problem
    END;

  VAR W: Texts.Writer;
    sum: LONGINT; problems: Problem;

    probline: ARRAY 5, 3800 OF CHAR; nbl, len: INTEGER;

  PROCEDURE parse1(process: ProcessProc);
    VAR S!: Texts.Scanner; line: INTEGER; p!, tail!: Problem;
  BEGIN problems := NIL; tail := NIL;
    Texts.OpenScanner(S, Oberon.Par.text, Oberon.Par.pos);
    Texts.Scan(S);
    WHILE S.class = Texts.Int DO line := S.line; p := problems;
      REPEAT
        IF line = 1 THEN NEW(p);
          IF tail = NIL THEN problems := p; tail := p ELSE tail.next := p; tail := p END
        END;
        ASSERT(S.class = Texts.Int); p.opd[line - 1] := S.i; p.nbd := line;
        Texts.Scan(S); p := p.next
      UNTIL S.line # line;
      line := S.line
    END;
    p := problems ;
    REPEAT ASSERT(S.class = Texts.Char);
      p.opr := S.c; p := p.next;
      Texts.Scan(S)
    UNTIL S.line # line;
    process
  END parse1;

  PROCEDURE doPart1;
    VAR p!: Problem; i: INTEGER; t: LONGINT;
  BEGIN p := problems;
    WHILE p # NIL DO
      IF p.opr = "*" THEN t := 1; FOR i := 0 TO p.nbd - 1 DO t := t*p.opd[i] END
      ELSE ASSERT(p.opr = "+"); t := 0; FOR i := 0 TO p.nbd - 1 DO INC(t, p.opd[i]) END
      END;
      INC(sum, t);
      p := p.next
    END
  END doPart1;

  PROCEDURE part1*;
  BEGIN sum := 0; parse1(doPart1);
    Texts.WriteString(W, "Partie 1: sum = "); Texts.WriteInt(W, sum, 1); Texts.WriteLn(W);
    Texts.Append(Oberon.Log, W.buf);
    problems := NIL
  END part1;

  PROCEDURE parse2(process: ProcessProc);
    VAR S!: Texts.Scanner; ch: CHAR; i: INTEGER;
  BEGIN Texts.OpenScanner(S, Oberon.Par.text, Oberon.Par.pos);
    nbl := 0; len := 0; i := 0; Texts.Read(S, ch); Texts.Read(S, ch);
    WHILE ((ch >= "0") & (ch <= "9")) OR (ch = " ") OR (ch = Texts.CR) OR (ch = "*") OR (ch = "+") DO
      IF ch = Texts.CR THEN INC(nbl); i := 0 ELSE probline[nbl, i] := ch; INC(i); IF i > len THEN len := i END END;
      Texts.Read(S, ch)
    END;
    process
  END parse2;

  PROCEDURE digit(i: INTEGER; VAR d: LONGINT): BOOLEAN;
    VAR nbd, j: INTEGER; ch: CHAR;
  BEGIN nbd := 0; d := 0;
    FOR j := 0 TO nbl - 2 DO ch := probline[j, i];
      IF (ch >= "0") & (ch <= "9") THEN d := d*10; INC(d, ORD(ch) - ORD("0")); INC(nbd) END
    END

    RETURN nbd > 0
  END digit;

  PROCEDURE doPart2;
    VAR i: INTEGER; t, d: LONGINT;
  BEGIN i := 0;
    WHILE i < len DO
      IF probline[nbl - 1, i] = "*" THEN t := 1;
        WHILE digit(i, d) DO t := t*d; INC(i) END
      ELSE ASSERT(probline[nbl - 1, i] = "+"); t := 0;
        WHILE digit(i, d) DO INC(t, d); INC(i) END
      END;
      INC(sum, t); INC(i)
    END
  END doPart2;

  PROCEDURE part2*;
  BEGIN sum := 0; parse2(doPart2);
    Texts.WriteString(W, "Partie 2: sum = "); Texts.WriteInt(W, sum, 1); Texts.WriteLn(W);
    Texts.Append(Oberon.Log, W.buf)
  END part2;

BEGIN Texts.OpenWriter(W)
END AOC2025Day6.

OM4P.Compile AOC2025Day6.Mod/s~
System.Free AOC2025Day6~


