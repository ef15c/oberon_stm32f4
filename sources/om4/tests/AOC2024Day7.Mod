MODULE AOC2024Day7;
  IMPORT Texts, TE := TextsEx2, Oberon;

  TYPE ProcessProc = PROCEDURE(r: LONGINT; p: ARRAY OF LONGINT; c: INTEGER);

  VAR W: Texts.Writer;
    s: LONGINT; pr: BYTE;

  PROCEDURE concat(l, r: LONGINT): LONGINT;
    VAR t: LONGINT;
  BEGIN ASSERT(l >= 0); ASSERT(r >= 0);
    t := r;
    REPEAT l := l*10; t := t DIV 10 UNTIL t = 0

    RETURN l+r
  END concat;

  PROCEDURE ReadL(VAR S: TE.Scanner): LONGINT;
    VAR r: LONGINT;
  BEGIN
 IF S.class = Texts.Int THEN r := S.i ELSE ASSERT(S.class = TE.LongInt); r := S.l END; TE.Scan(S)
    RETURN r
  END ReadL;

  PROCEDURE parse(process: ProcessProc);
    VAR S!: TE.Scanner;
    r: LONGINT; p: ARRAY 20 OF LONGINT; i, line: INTEGER;
  BEGIN Texts.OpenScanner(S, Oberon.Par.text, Oberon.Par.pos); TE.Scan(S);
    WHILE S.class IN {Texts.Int, TE.LongInt} DO line := S.line;
      r := ReadL(S); IF S.class = Texts.Char THEN TE.Scan(S) END;
      i := 0; WHILE (line = S.line) & (S.class IN {Texts.Int, TE.LongInt}) DO p[i] := ReadL(S); INC(i) END;
      process(r, p, i)
    END
  END parse;

  PROCEDURE doPart1(r: LONGINT; p: ARRAY OF LONGINT; c: INTEGER);
    VAR i, k: INTEGER; ir: ARRAY 20 OF LONGINT; op: ARRAY 20 OF INTEGER;
  BEGIN (*Texts.WriteString(W, "r="); TE.WriteLongInt(W, r, 1); Texts.WriteString(W, ": ");
    FOR i := 0 TO c-1 DO TE.WriteLongInt(W, p[i], 5) END;
    Texts.WriteLn(W); Texts.Append(Oberon.Log, W.buf)*)
    ASSERT(c > 0);
    k := 1; ir[0] := p[0]; FOR i := 0 TO LEN(op) - 1 DO op[i] := 0 END;
    WHILE k > 0 DO
      IF k >= c THEN IF ir[k - 1] = r THEN INC(s, r); k := 0 END; DEC(k)
      ELSE INC(op[k]);
        IF op[k] > 2 THEN
          op[k] := 0; DEC(k)
        ELSE
          IF op[k] = 1 THEN ir[k] := ir[k - 1] + p[k] ELSE ir[k] := ir[k - 1] * p[k] END; INC(k)
        END
      END
    END
  END doPart1;

  PROCEDURE part1*;
  BEGIN s := 0; parse(doPart1);
    Texts.WriteString(W, "Partie 1: somme = "); TE.WriteLongInt(W, s, 1);
    Texts.WriteLn(W); Texts.Append(Oberon.Log, W.buf)
  END part1;

  PROCEDURE doPart2(r: LONGINT; p: ARRAY OF LONGINT; c: INTEGER);
    VAR i, k: INTEGER; ir: ARRAY 20 OF LONGINT; op: ARRAY 20 OF INTEGER;
  BEGIN ASSERT(c > 0); LED(pr); INC(pr);
    k := 1; ir[0] := p[0]; FOR i := 0 TO LEN(op) - 1 DO op[i] := 0 END;
    WHILE k > 0 DO
      IF k >= c THEN IF ir[k - 1] = r THEN INC(s, r); k := 0 END; DEC(k)
      ELSE INC(op[k]);
        IF op[k] > 3 THEN
          op[k] := 0; DEC(k)
        ELSE
          IF op[k] = 1 THEN ir[k] := ir[k - 1] + p[k]
          ELSIF op[k] = 2 THEN ir[k] := ir[k - 1] * p[k]
          ELSE ir[k] := concat(ir[k - 1], p[k])
          END; INC(k)
        END
      END
    END
  END doPart2;

  PROCEDURE part2*;
  BEGIN s := 0; parse(doPart2);
    Texts.WriteString(W, "Partie 2: somme = "); TE.WriteLongInt(W, s, 1);
    Texts.WriteLn(W); Texts.Append(Oberon.Log, W.buf)
  END part2;

BEGIN Texts.OpenWriter(W)
END AOC2024Day7.

